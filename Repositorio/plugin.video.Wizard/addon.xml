	   version="1.0.1" 
	   provider-name="Jose bilbao">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable video</provides>
		
		<extension point="xbmc.addon.repository" name="Mi repositorio">
        <info>https://raw.githubusercontent.com/alkiber/repokeppler/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/alkiber/repokeppler/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/alkiber/repokeppler/master/Repositorio/</datadir>
    </extension>
		
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Wizard</summary>
    <description>Instalador inteligente creado y modificado por jose kobra para una actualizacion optima 2018 by keppler  </description>
    <platform>all</platform>
  </extension>
</addon>
